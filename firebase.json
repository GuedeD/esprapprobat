{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/",
        "destination": "https://www.approbat.com",
        "type": 301
      },
      {
        "source": "/(.*)",
        "destination": "https://www.approbat.com",
        "type": 301
      }
    ]
  }
}
